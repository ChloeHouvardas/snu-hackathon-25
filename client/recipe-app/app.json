{
  "expo": {
    "name": "Yorigo",
    "slug": "yorigo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Yorigo_icon_dark.png",
    "scheme": "recipeapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/Yorigo_icon_dark.png",
      "bundleIdentifier": "com.yorigo.yorigo"
    },
    "android": {
      "icon": "./assets/images/Yorigo_icon_dark.png",
      "adaptiveIcon": {
        "backgroundColor": "#FF6900",
        "foregroundImage": "./assets/images/Yorigo_icon_light.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.yorigo.yorigo"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Yorigo_icon_dark.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Yorigo_icon_dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/Yorigo_icon_light.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
